{
  "name": "supernote-typescript",
  "version": "0.3.0",
  "description": "Supernote file-format support.",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test-watch": "jest --watch",
    "test-mirror": "jest --watch -t mirror",
    "coverage": "jest --coverage",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/philips/supernote-typescript.git"
  },
  "keywords": [
    "supernote",
    "typescript",
    "library"
  ],
  "author": "Brandon Philips",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/philips/supernote-typescript/issues"
  },
  "homepage": "https://gitlab.com/philips/supernote-typescript",
  "devDependencies": {
    "@types/color": "^3.0.3",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.5",
    "@types/sharp": "^0.30.5",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.57.0",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^28.1.3",
    "prettier": "^3.2.5",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4",
    "v8-profiler-next": "^1.10.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1",
      "^~/(.*)$": "<rootDir>/src/$1"
    },
    "testRegex": "^.+\\.test\\.ts$"
  },
  "dependencies": {
    "color": "^4.2.3",
    "fs-extra": "^10.1.0",
    "image-js": "^0.35.5"
  }
}
